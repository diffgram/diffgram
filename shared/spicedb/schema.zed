definition diffgram/user {}

definition diffgram/dataset {
    relation reader: diffgram/user
    relation writer: diffgram/user

    permission read = reader + writer
    permission write = writer
}

definition diffgram/file {
    relation reader: diffgram/user
    relation writer: diffgram/user
    relation dataset: diffgram/dataset

    permission read = reader + writer + dataset->reader
    permission write = writer + dataset->reader
}

